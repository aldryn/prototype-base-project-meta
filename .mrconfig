
[base-docker]
checkout = git clone 'git@github.com:aldryncore/base-docker.git' 'base-docker' && cd base-docker && git checkout -b feature/official-python origin/feature/official-python

[base-project]
checkout = git clone 'git@github.com:aldryn/cloud-base-project.git' 'base-project' && cd base-project && git checkout -b feature/minimal origin/feature/minimal

[project-template]
checkout = git clone 'git@github.com:aldryn/prototype-aldryn-project-template.git' 'project-template' && cd project-template && git checkout -b feature/prototype origin/feature/prototype
